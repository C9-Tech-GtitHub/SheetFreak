# SheetFreak Configuration Example
# Copy this to .env and fill in your values

# Path to your Google Cloud service account JSON credentials file
SHEETFREAK_CREDENTIALS=/path/to/service-account.json

# Optional: Default spreadsheet ID to work with
SHEETFREAK_DEFAULT_SHEET=

# Optional: Output format (table, json, csv)
SHEETFREAK_OUTPUT_FORMAT=table

# Optional: Enable verbose error messages
SHEETFREAK_VERBOSE_ERRORS=true
